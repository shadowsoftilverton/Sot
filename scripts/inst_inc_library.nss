            nRand -= CREATURE_LIBRARY_WEIGHT_CORMYR_HULLACK_BUGBEAR;

            break;
        case CREATURE_LIBRARY_REGION_CORMYR_KINGSFOREST:
            nTotal = CREATURE_LIBRARY_WEIGHT_CORMYR_KINGSFOREST_ANIMAL_RAT +
                     CREATURE_LIBRARY_WEIGHT_CORMYR_KINGSFOREST_ANIMAL_WOLF +
