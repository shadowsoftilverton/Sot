// Returns standard area parts (linear portions with an entrance and exit).
string GetStandardAreaLibrary(int nAreaType);

// Returns capped area parts (portions with only an entrance).
string GetCappedAreaLibrary(int nAreaType);

// Returns a creature library array corresponding to a type of creature.
string GetCreatureLibrary(int nCreatureType);

// Returns a creature library array by randomly selecting from several
// available per region.
string GetCreatureLibraryByRegion(int nRegion);

//----------------------------------------------------------------------------//

string GetStandardAreaLibrary(int nAreaType) {
    switch(nAreaType) {
        case AREA_LIBRARY_TYPE_CAVE:    return AREA_LIBRARY_ARRAY_CAVE;    break;
        case AREA_LIBRARY_TYPE_CRYPT:   return AREA_LIBRARY_ARRAY_CRYPT;   break;
        case AREA_LIBRARY_TYPE_DUNGEON: return AREA_LIBRARY_ARRAY_DUNGEON; break;
    }

    return "";
}

string GetCappedAreaLibrary(int nAreaType) {
    switch(nAreaType) {
        case AREA_LIBRARY_TYPE_CAVE:    return AREA_LIBRARY_ARRAY_CAP_CAVE;    break;
        case AREA_LIBRARY_TYPE_CRYPT:   return AREA_LIBRARY_ARRAY_CAP_CRYPT;   break;
        case AREA_LIBRARY_TYPE_DUNGEON: return AREA_LIBRARY_ARRAY_CAP_DUNGEON; break;
    }

    return "";
}

string GetCreatureLibrary(int nCreatureType) {
    switch(nCreatureType) {
        case CREATURE_LIBRARY_TYPE_ANIMAL_RAT:     return CREATURE_LIBRARY_ARRAY_ANIMAL_RAT;     break;
        case CREATURE_LIBRARY_TYPE_ANINAL_WOLF:    return CREATURE_LIBRARY_ARRAY_ANIMAL_WOLF;    break;
        case CREATURE_LIBRARY_TYPE_ANIMAL_BEAR:    return CREATURE_LIBRARY_ARRAY_ANIMAL_BEAR;    break;
        case CREATURE_LIBRARY_TYPE_ANIMAL_BADGER:  return CREATURE_LIBRARY_ARRAY_ANIMAL_BADGER;  break;
        case CREATURE_LIBRARY_TYPE_GOBLIN:         return CREATURE_LIBRARY_ARRAY_GOBLIN;         break;
        case CREATURE_LIBRARY_TYPE_ORC:            return CREATURE_LIBRARY_ARRAY_ORC;            break;
        case CREATURE_LIBRARY_TYPE_GNOLL:          return CREATURE_LIBRARY_ARRAY_GNOLL;          break;
        case CREATURE_LIBRARY_TYPE_OGRE:           return CREATURE_LIBRARY_ARRAY_OGRE;           break;
        case CREATURE_LIBRARY_TYPE_TROLL:          return CREATURE_LIBRARY_ARRAY_TROLL;          break;
        case CREATURE_LIBRARY_TYPE_UNDEAD:         return CREATURE_LIBRARY_ARRAY_UNDEAD;         break;
        case CREATURE_LIBRARY_TYPE_SHADOWS:        return CREATURE_LIBRARY_ARRAY_SHADOWS;        break;
        case CREATURE_LIBRARY_TYPE_OWLBEAR:        return CREATURE_LIBRARY_ARRAY_OWLBEAR;        break;
        case CREATURE_LIBRARY_TYPE_WYVERN:         return CREATURE_LIBRARY_ARRAY_WYVERN;         break;
        case CREATURE_LIBRARY_TYPE_ETIN:           return CREATURE_LIBRARY_ARRAY_ETIN;           break;
        case CREATURE_LIBRARY_TYPE_GIANT_HILL:     return CREATURE_LIBRARY_ARRAY_GIANT_HILL;     break;
        case CREATURE_LIBRARY_TYPE_GIANT_STONE:    return CREATURE_LIBRARY_ARRAY_GIANT_STONE;    break;
        case CREATURE_LIBRARY_TYPE_GIANT_FIRE:     return CREATURE_LIBRARY_ARRAY_GIANT_FIRE;     break;
        case CREATURE_LIBRARY_TYPE_GIANT_FROST:    return CREATURE_LIBRARY_ARRAY_GIANT_FROST;    break;
    }

    return "";
}

string GetCreatureLibraryByRegion(int nRegion) {
    int nRand;
    int nTotal;

    switch(nRegion) {
        case CREATURE_LIBRARY_REGION_CORMYR_HULLACK:
            nTotal = CREATURE_LIBRARY_WEIGHT_CORMYR_HULLACK_ANIMAL_RAT +
                     CREATURE_LIBRARY_WEIGHT_CORMYR_HULLACK_ANIMAL_WOLF +
                     CREATURE_LIBRARY_WEIGHT_CORMYR_HULLACK_ANIMAL_BEAR +
                     CREATURE_LIBRARY_WEIGHT_CORMYR_HULLACK_ANIMAL_BADGER +
                     CREATURE_LIBRARY_WEIGHT_CORMYR_HULLACK_GOBLIN +
                     CREATURE_LIBRARY_WEIGHT_CORMYR_HULLACK_ORC +
                     CREATURE_LIBRARY_WEIGHT_CORMYR_HULLACK_GNOLL +
                     CREATURE_LIBRARY_WEIGHT_CORMYR_HULLACK_TROLL +
                     CREATURE_LIBRARY_WEIGHT_CORMYR_HULLACK_OWLBEAR +
                     CREATURE_LIBRARY_WEIGHT_CORMYR_HULLACK_WYVERN +
                     CREATURE_LIBRARY_WEIGHT_CORMYR_HULLACK_ETIN +
                     CREATURE_LIBRARY_WEIGHT_CORMYR_HULLACK_BUGBEAR;

            nRand = Random(nTotal);

            if(nRand < CREATURE_LIBRARY_WEIGHT_CORMYR_HULLACK_ANIMAL_RAT) return GetCreatureLibrary(CREATURE_LIBRARY_TYPE_ANIMAL_RAT);
            nRand -= CREATURE_LIBRARY_WEIGHT_CORMYR_HULLACK_ANIMAL_RAT;

            if(nRand < CREATURE_LIBRARY_WEIGHT_CORMYR_HULLACK_ANIMAL_WOLF) return GetCreatureLibrary(CREATURE_LIBRARY_TYPE_ANINAL_WOLF);
            nRand -= CREATURE_LIBRARY_WEIGHT_CORMYR_HULLACK_ANIMAL_WOLF;

