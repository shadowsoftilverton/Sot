            if(nRand < CREATURE_LIBRARY_WEIGHT_CORMYR_HULLACK_OWLBEAR) return GetCreatureLibrary(CREATURE_LIBRARY_TYPE_OWLBEAR);
            nRand -= CREATURE_LIBRARY_WEIGHT_CORMYR_HULLACK_OWLBEAR;

            if(nRand < CREATURE_LIBRARY_WEIGHT_CORMYR_HULLACK_WYVERN) return GetCreatureLibrary(CREATURE_LIBRARY_TYPE_WYVERN);
